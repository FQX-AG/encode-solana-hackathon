    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.0.3",
    "prettier": "^2.8.8",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.3.3"
  }
}
