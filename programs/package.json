  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "build": "anchor build && cp -R target/types/* src/types && tsc -P tsconfig.cjs.json && tsc -P tsconfig.esm.json",
    "build:ts": "tsc -P tsconfig.cjs.json && tsc -P tsconfig.esm.json",
    "clean": "rm -rf dist",
    "test": "anchor test",
    "test-debug": "DEBUG=true BLUEBIRD_LONG_STACK_TRACES=1 anchor test",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "upgrade:structured-product:devnet": "anchor upgrade --provider.cluster devnet --program-id=GYFmKqbpYHUrML3BstU9VUnVdEE6ho9tzVJzs1DAR5iz ./target/deploy/structured_product.so",
    "upgrade:treasury-wallet:devnet": "anchor upgrade --provider.cluster devnet --program-id=3DUqJ4S1dUoKzC77NmJXq2wiDqwR3NoNkEwtkFU4SaY3 ./target/deploy/treasury_wallet.so",
    "upgrade:transferhook:devnet": "anchor upgrade --provider.cluster devnet --program-id=6sGAcb6vw8bhcVNPv5pMEhr3dXyeYoX2X89S3NkEaaJP ./target/deploy/transfer_snapshot_hook.so"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/spl-token": "0.3.11",
    "@solana/web3.js": "1.89.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
